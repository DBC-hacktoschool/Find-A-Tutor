<form class="" action="events/search" method="get">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label for="search-term">Subject?</label>
  <input id="search-term" type="text" name="search[term]" placeholder="What subject?">

  <label for="search-zip">Zipcode?</label>
  <input id="search-zip" type="text" name="search[zip]" placeholder="Zipcode?">

  <button type="submit" name="button">Add</button>
</form>
