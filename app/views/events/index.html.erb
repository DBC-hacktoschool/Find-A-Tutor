<% @page_title = 'Upcoming Tutoring Session' %>
<%= render partial: 'layouts/header', locals: {page_title: @page_title} %>

<div>
  <% @events.each do |event| %>
    <div class='bg-g img-rounded h w'>
    <h2><%= link_to event.name, event_path(event)%></h2>
    <%= link_to 'Edit', edit_event_path(event.id), class:"btn btn-primary"  %> | <%= link_to 'Delete', event_path(event.id), class:"btn btn-primary", method: :delete, data: {confirm: 'Are you sure?'} %><br>
  </div>
  <% end %>
</div>
</div>
