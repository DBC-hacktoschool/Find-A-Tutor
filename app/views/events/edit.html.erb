<h2>Edit Event</h2>
<%= form_for @event do |f| %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %><br>
  </div>

  <div class="form-group">
    <%= f.label :attendees %>
    <%= f.text_field :attendees %><br>
  </div>

  <div class="form-group">
    <%= f.label :time_start %>
    <%= f.text_field :time_start %><br>
  </div>

  <div class="form-group">
    <%= f.label :time_end %>
    <%= f.text_field :time_end %><br>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_field :description %><br>
  </div>

  <div class="form-group">
    <%= f.label :subject_id %>
    <%= f.text_field :subject_id %><br>
  </div>

  <div class="form-group">
    <%= f.label :user_id %>
    <%= f.text_field :user_id %><br>
  </div>

  <div class="form-group">
    <%= f.label :location_id %>
    <%= f.text_field :location_id %><br>
  </div>

  <div class="form-group">
    <%= f.label :group_id %>
    <%= f.text_field :group_id %><br>
  </div>

  <%= f.submit "Submit", class:'btn btn-primary' %>
  <%= link_to "Cancel", events_path, class:"btn btn-default" %>

<% end %>